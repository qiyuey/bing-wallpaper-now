{
  "name": "bing-wallpaper-now",
  "private": true,
  "version": "0.3.8-0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:md": "markdownlint \"*.md\" \"docs/**/*.md\" --ignore node_modules --ignore src-tauri/target",
    "lint:md:fix": "markdownlint \"*.md\" \"docs/**/*.md\" --ignore node_modules --ignore src-tauri/target --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test:rust": "cargo test --manifest-path src-tauri/Cargo.toml",
    "test:frontend": "vitest run",
    "test": "cargo test --manifest-path src-tauri/Cargo.toml && vitest run"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-autostart": "^2.5.1",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-opener": "^2.5.2",
    "@tauri-apps/plugin-store": "^2.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-window": "^2.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tauri-apps/cli": "^2.9.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.5",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.0.1",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.5"
  },
  "packageManager": "pnpm@10.19.0"
}
